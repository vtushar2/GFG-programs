class Solution {
    ArrayList<Integer> valueEqualToIndex(int arr[], int n) {
        // code here
        ArrayList<Integer> a1=new ArrayList<>();
        for(int i=0;i<n;i++)
        {
            if(arr[i]==i+1)
            {
                a1.add(i+1);
            }
        }
        return a1;
    }
}
